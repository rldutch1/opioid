function checkDiscount()
{
totprice = 0;
var price = new Array();
price[0] = 1.00;
price[1] = 5.00;
price[2] = 1.00;
price[3] = 1.00;
var quantity = new Array();
quantity[0] = parseInt(form1.qty1.value);
quantity[1] = parseInt(form1.qty2.value);
quantity[2] = parseInt(form1.qty3.value);
quantity[3] = parseInt(form1.qty4.value);
for(i=0;i<4;i++)
{
totprice = totprice + quantity[i] * price[i];
}
//memtype = form1.membertype.value;
dispdiscnt = '';
switch(memtype)
{
case "gold":
discount=15;
dispdiscnt = discount + " percent discount for Gold Card Members";
break;
case "silver":
discount=10;
dispdiscnt = discount + " percent discount for Silver Card Members";
break;
case "privilege":
discount=5; dispdiscnt = discount + " percent discount for Privilege Card Members";
break;
default:
discount=0;
}
disprice = totprice - totprice * discount/100;
}
}
